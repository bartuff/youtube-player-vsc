{
  "name": "youtube-player",
  "displayName": "YouTube Player",
  "description": "Reproductor de YouTube para VS Code",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": ""
  },
  "publisher": "srjson",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "youtube-player.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "youtubePlayer.addVideo",
        "title": "Add YouTube Video",
        "icon": "$(add)"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "youtubePlayerView",
          "name": "YouTube Player",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "youtubePlayer.addVideo",
          "when": "view == youtubePlayerView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "license": "MIT"
}
